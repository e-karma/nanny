{{#store-layout}}
  <div class="product-listing">
  {{#each model as |product|}}
    <div class="product-card">
      <img src={{product.images.firstObject.imageUrl}}>
      <p style="text-align: center;">
        <strong>{{product.title}}</strong> / Description<br/>
        ${{product.price}}
      </p>
    </div>
  {{/each}}
</div>
{{/store-layout}}
